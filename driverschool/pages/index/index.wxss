/**index.wxss**/
@font-face {
  font-family: 'iconfont';  /* project id 441434 */
  src: url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.eot');
  src: url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.woff') format('woff'),
  url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.svg#iconfont') format('svg');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shijian:before { content: "\e600"; }

.icon-bofang:before { content: "\e630"; }

.icon-qianjin-copy:before { content: "\e603"; }

.icon-zhuye:before { content: "\e604"; }

.icon-tupian:before { content: "\e61e"; }

.icon-dingwei:before { content: "\e7a1"; }

.icon-dianhua:before { content: "\e6bc"; }

.index-row{
  padding:30rpx 0;
  font-size: 14px;
  border-bottom: 1px dashed #dbdbdb;
}
.index-row:last-child{
  border-bottom-width: 0px;
}

.index-tr{
  text-align: right;
}

.flex-grow-3{
  flex-grow:58
}

.flex-grow-1{
  flex-grow: 1
}

.index-header{
  display: flex;
  flex-direction: row;
  justify-content:space-between;
}
.index-header-title{
  flex-basis:auto;
  font-size: 21px;
  font-weight: bold;
  letter-spacing: 4rpx;
}
.index-header-title .desc{
  display: block;
  font-size: 14px;
  padding: 10rpx 0;
  font-weight: 400;
  color: #999;
}
.index-header-logo{
  width: 148rpx;
  height: 108rpx;
}

.index-introduce {
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  padding-top: 30rpx;
  font-size: 14px;
  color: #777;
}
.index-introduce-slogan{
  height:30rpx;
  line-height:2.3;
}
.index-introduce-navigater{
  margin: 0;
  padding: 10rpx 25rpx;
  border: 1px solid #26A128;
  color: #26A128;
  font-size: 11px;
  border-radius: 5px;
}

.index-propaganda{
  line-height: 1.5;
  font-size: 12px;
  font-weight: bold;
  color: #777;
}

.index-news{
  color:red;
  font-weight:bold;
}

.index-schoolInfo{
  padding-top: 0;
}
.index-schoolInfo-item{
  padding: 30rpx 0;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
}

.index-schoolInfo-icon{
  color: #dbdbdb;
}
.index-schoolInfo-image{
  display: flex;
  flex-direction:row;
  flex-wrap:wrap;
  justify-content:center;
}
.index-schoolInfo-image image{
  width: 220rpx;
  height: 165rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  flex-basis:auto;
}
.index-schoolInfo-item-middle{
  margin-left: 20rpx;
}
.index-schoolInfo-item-more{
  color:#292A86;
}