/**index.wxss**/
@font-face {
  font-family: 'iconfont';  /* project id 441434 */
  src: url('//at.alicdn.com/t/font_441434_z19qxn5pdum78pvi.eot');
  src: url('//at.alicdn.com/t/font_441434_z19qxn5pdum78pvi.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_441434_z19qxn5pdum78pvi.woff') format('woff'),
  url('//at.alicdn.com/t/font_441434_z19qxn5pdum78pvi.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_441434_z19qxn5pdum78pvi.svg#iconfont') format('svg');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shijian:before { content: "\e600"; }

.icon-bofang:before { content: "\e630"; }

.icon-qianjin-copy:before { content: "\e603"; }

.icon-tupian:before { content: "\e61e"; }

.icon-dingwei:before { content: "\e7a1"; }

.icon-dianhua:before { content: "\e6bc"; }

.icon-laba:before { content: "\e671"; }

.index-row{
  padding:30rpx 0;
  font-size: 14px;
  border-bottom: 1px dashed #dbdbdb;
}
.index-row:last-child{
  border-bottom-width: 0px;
}

.index-tr{
  text-align: right;
}

.flex-grow-3{
  flex-grow:58
}

.flex-grow-1{
  flex-grow: 1
}

.index-header{
  display: flex;
  flex-direction: row;
  justify-content:space-between;
}
.index-header-title{
  flex-basis:auto;
  font-size: 21px;
  font-weight: bold;
  letter-spacing: 4rpx;
}
.index-header-title .desc{
  display: block;
  font-size: 14px;
  padding: 10rpx 0;
  font-weight: 400;
  color: #999;
}
.index-header-logo{
  width: 148rpx;
  height: 108rpx;
}

.index-introduce {
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  padding-top: 30rpx;
  font-size: 14px;
  color: #777;
}
.index-introduce-slogan{
  height:30rpx;
  line-height:2.3;
}
.index-introduce-navigater{
  margin: 0;
  padding: 10rpx 25rpx;
  border: 1px solid #26A128;
  color: #26A128;
  font-size: 11px;
  border-radius: 5px;
}

.index-propaganda{
  line-height: 1.5;
  font-size: 12px;
  font-weight: bold;
}
.index-propaganda view{
  margin-left: 45rpx;
  color: #777;
}

.index-news {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  color: #777;
}
.index-news .icon-red{
  margin-right:25rpx;
  color:red;
  line-height:2.3;
  font-size:12px;
}
.index-news-middle{
  font-size: 12px;
  line-height: 2.3;
  font-weight: bold;
}
.index-news button{
  padding:0 10rpx;
  font-size:12px;
  color: #0C924D;
  border: 1px solid #0C924D;
  background-color: #fff;
}

.index-schoolInfo{
  padding-top: 0;
}
.index-schoolInfo-item{
  padding: 30rpx 0;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  font-size: 13px;
}

.index-schoolInfo-icon{
  color: #dbdbdb;
}
.index-schoolInfo-image{
  display: flex;
  flex-direction:row;
  flex-wrap:wrap;
  justify-content:center;
}
.index-schoolInfo-image image{
  width: 220rpx;
  height: 165rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  flex-basis:auto;
}
.index-schoolInfo-item-middle{
  margin-left: 20rpx;
}
.index-schoolInfo-item-more{
  color:#292A86;
}