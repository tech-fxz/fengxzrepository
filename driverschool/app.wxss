/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.pages{
  padding: 30rpx;
  color: #333;
}

@font-face {
  font-family: 'iconfont';  /* project id 441434 */
  src: url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.eot');
  src: url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.woff') format('woff'),
  url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_441434_8hc0mx1h3373c8fr.svg#iconfont') format('svg');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

